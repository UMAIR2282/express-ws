{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "Test Driven Development using Express Js",
  "main": "app.js",
  "scripts": {
    "start": "nodemon app",
    "test": "jest --watch",
    "lint": "eslint ."
  },
  "keywords": [
    "TDD",
    "ExpressJs",
    "NodeJs",
    "TypeScript"
  ],
  "author": "Umair Ali",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.2"
  },
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq": "warn",
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120
  }
}
